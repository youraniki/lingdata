    <TIER LINGUISTIC_TYPE_REF="translation" PARENT_REF="text@dpv1973m"
        PARTICIPANT="@dpv1973m" TIER_ID="translate@dpv1973m">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a24" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>here</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a25" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>And I am wondering, is it worth giving such a task, for example, for a State Exam or not?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a26" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>No</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a27" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>It is actually a good question, because...</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a28" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE>... If we give it at the Oral Geometry Olympiad, an experienced persom will say</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a29" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>Let's see how it works</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a30" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>Lots of circumfences that tangence</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a31" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>Unfortunately, I can't start GeoGebra and move it</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a32" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>But that looks quite beautiful, everything's moving</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a33" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>And so</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a34" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>Circumfences, for example, the green ones</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a35" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>They will gargence constantly. How does it work?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a36" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE>That red circumfence is inscribed</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a37" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE>That bold white point is a midpoit of this segment</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a38" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>And then we construct</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a39" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>we construct a circumfence with this radius</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a40" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE>And the second circumfence, the green one, is constructed on the side of the triangle as on the diameter.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a41" ANNOTATION_REF="a20">
                <ANNOTATION_VALUE>the others are constructed similarly</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a42" ANNOTATION_REF="a21">
                <ANNOTATION_VALUE>When you show this picture to students</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a43" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE>they say there is another big circumfence that tangence all the others</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a44" ANNOTATION_REF="a22">
                <ANNOTATION_VALUE>What is this big circumfence?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
